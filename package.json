{
  "name": "broadclass",
  "version": "1.0.0",
  "private": true,
  "main": "public/electron.js",
  "author": "broadClass",
  "description": "英荔播课",
  "homepage": ".",
  "productName": "英荔播课",
  "dependencies": {
    "@sentry/electron": "^1.0.0",
    "electron-log": "^1.3.0",
    "electron-updater": "^4.0.6",
    "moment": "^2.24.0",
    "zegoliveroom": "latest"
  },
  "devDependencies": {
    "@antv/data-set": "^0.10.2",
    "@antv/g2": "^3.5.11",
    "@babel/core": "7.4.3",
    "@babel/plugin-proposal-decorators": "^7.6.0",
    "@netless/react-tool-box": "^1.0.3",
    "@svgr/webpack": "4.1.0",
    "@types/react": "^16.9.11",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "antd": "^4.1.5",
    "app-builder-bin": "^3.4.4",
    "axios": "^0.19.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "^1.12.1",
    "babel-plugin-named-asset-import": "^0.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-react-app": "^9.0.0",
    "bizcharts": "^3.5.5",
    "camelcase": "^5.2.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "dva": "^2.4.1",
    "electron": "5.0.12",
    "electron-builder": "^20.28.4",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-proxy-middleware": "^0.20.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.7.1",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.7.1",
    "jest-watch-typeahead": "0.3.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "0.5.0",
    "numeral": "^2.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.6.0",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-app-polyfill": "^1.0.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-quill": "^1.3.3",
    "react-router-dom": "^5.0.1",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "semver": "6.0.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.3",
    "ts-loader": "^6.2.1",
    "ts-pnp": "1.1.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-dev-server": "3.2.1",
    "webpack-manifest-plugin": "2.0.4",
    "white-react-sdk": "^2.7.5",
    "workbox-webpack-plugin": "4.2.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node --max-old-space-size=8000 scripts/build.js",
    "test": "node scripts/test.js",
    "format": "prettier -c --write \"**/*\"",
    "electron-dev": "electron . development",
    "electron": "npm run build && electron . production",
    "package": "npm run build && rm -rf dist && electron-builder",
    "postinstall2": "electron-builder install-app-deps",
    "postinstall": "node node_modules/zegodown/bin/zegodown -v 5.0.12",
    "pkg": "npm run build && node ./public/pkg.js",
    "dist": "npm run dist:mac",
    "dist:mac": "build --platform darwin --arch x64 ",
    "dist:win": "build --platform win32 --arch ia32 ",
    "dist:linux": "build --platform linux --arch x64 "
  },
  "build": {
    "appId": "cn.elite.broadclass.desktop",
    "productName": "英荔播课",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "extraResources": [
      "./node_modules/zegoliveroom/**"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://yingliboke.oss-cn-shenzhen.aliyuncs.com/app/"
      }
    ],
    "files": [
      "./build/**/*",
      "./public/electron.js"
    ],
    "mac": {
      "category": "yingliboke.desktop",
      "target": "dmg",
      "icon": "./public/logo_icon_512.png"
    },
    "dmg": {
      "icon": "./public/logo_icon_512.png",
      "background": "./public/logo_icon_background.png",
      "iconSize": 80,
      "iconTextSize": 12,
      "title": "${productName} ${version}",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      }
    },
    "win": {
      "icon": "./public/logo_icon_512.ico",
      "target": "nsis",
      "legalTrademarks": "Eyas Personal"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": false,
      "oneClick": true,
      "menuCategory": false,
      "allowElevation": false,
      "installerIcon": "./public/logo_icon_512.ico",
      "uninstallerIcon": "./public/logo_icon_512.ico",
      "installerHeader": "./public/logo_icon_512.ico",
      "installerHeaderIcon": "./public/logo_icon_512.ico",
      "installerSidebar": "./public/logo_icon_512.ico",
      "uninstallerSidebar": "./public/logo_icon_512.ico",
      "uninstallDisplayName": "${productName} ${version}",
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "broadclass"
    }
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-unused-expressions": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ]
    ],
    "presets": [
      "react-app"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "__npminstall_done": false
}
